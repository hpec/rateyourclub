%head
    %title Berkeley Clubs
    {% compress css %}
        %link{ 'href' : "{% static 'css/bootstrap.min.css' %}", 'rel' : 'stylesheet', 'type' : 'text/css' }
        %link{ 'href' : "{% static 'css/style.css' %}", 'rel' : 'stylesheet', 'type' : 'text/css' }
    {% endcompress %}
    - include_jquery_libs
%body
    .navbar.navbar-fixed-top
        .navbar-inner
            .container
                %a{'class':'brand', 'href': "{% url 'club_list_view' %}"} Berkeley Clubs
                %form{'class':'navbar-search pull-left', 'action':'/clubs/'}
                    %div{'class':'input-prepend input-append'}
                        %span{'class':'add-on'}
                            %i{'class':'icon-search'}
                        %input{'type':'text','class':'','placeholder':'Search', 'name':'q'}
                        %button{'class':'btn', 'type':'submit'} Search
                        %button{'class':'btn', 'type':'button'} Categories
    .container
        - block content
