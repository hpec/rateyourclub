- load static
- load selectable_tags
- include_jquery_libs
%head
    {{ form.media.css }}
    {{ form.media.js }} 
    %link{ 'href' : "{% static 'css/bootstrap.min.css' %}", 'rel' : 'stylesheet', 'type' : 'text/css' }
    %title
        Add Review

%div{'class':'container'}
    %div{'class':'row'}
        %form{ 'method':"post", 'action':"", 'class':"form-horizontal"}
            - csrf_token
            - for field in form
                %p
                    {{ field.label_tag }} {{ field }}
            
            %button{'type':"submit", 'class':"btn-primary"}
                Submit
